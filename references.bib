	doi = {10.1371/journal.pone.0296053},
	journal = {PLoS One},
	journal-full = {PloS one},
	mesh = {Male; Humans; Hepacivirus; HIV Infections; Homosexuality, Male; Phylogeny; Sexual and Gender Minorities; Hepatitis C; China; Genotype},
