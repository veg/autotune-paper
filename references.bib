	pmid = {32228123},
	title = {Using an {Established} {Outbreak} {Response} {Plan} and {Molecular} {Epidemiology} {Methods} in an {HIV} {Transmission} {Cluster} {Investigation}, {Tennessee}, {January}-{June} 2017},
