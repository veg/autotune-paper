	number = {3},
	pages = {e0283031},
	pmc = {PMC10065240},
	pmid = {37000807},
	pst = {epublish},
	title = {Inferring potential non-disclosed men who have sex with men among self-reported heterosexual men with HIV in Southwest China: A genetic network study},
